{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/room",
        "destination": "/room.html"
      },
      {
        "source": "**",
        "destination": "/login.html"
      } ,{
        "source": "/agendamentos",
        "destination": "public/agendamentos/agendamentos.html"
      },
      {
        "source": "/mapa",
        "destination": "/mapa.html"
      },
      {
        "source": "/room",
        "destination": "/room.html"
      }
    ]
  }
}
